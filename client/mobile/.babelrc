{
    "plugins": [
        [
            "module-resolver",
            {
                "cwd": "babelrc",
                "root": [
                    "./src"
                ],
                "extensions": [
                    ".js",
                    ".ios.js",
                    ".android.js"
                ],
                "alias": {
                    "_assets": "./src/assets",
                    "_components": "./src/components",
                    "_constants": "./src/constants",
                    "_hooks":"./src/hooks",
                    "_navigations": "./src/navigations",
                    "_scenes": "./src/scenes",
                    "_services": "./src/services",
                    "_styles": "./src/styles",
                    "_utils": "./src/utils"
                }
            }
        ]
    ]
}